echo "Building libRcpp.a in RcppSrc..."

: ${R_HOME}
if test -z "${R_HOME}"; then
   R_HOME=`${R_EXE} RHOME`		
fi
if test -z "${R_HOME}"; then
   R_HOME=`R RHOME`		
fi
if test -z "${R_HOME}"; then
    echo "Could not determine R home directory"
    exit 1
fi
cd RcppSrc
make -f Makefile.win
cd ..
if test \! -d inst
then
    mkdir inst
fi
cp RcppSrc/LICENSE-Rcpp.txt inst
